{
  "name": "my-better-t-app",
  "private": true,
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "type": "module",
  "scripts": {
    "dev": "npm run dev --workspaces",
    "build": "npm run build --workspaces",
    "check-types": "npm run check-types --workspaces",
    "dev:native": "npm run dev --workspace native",
    "dev:web": "npm run dev --workspace web",
    "dev:server": "npm run dev --workspace @my-better-t-app/backend",
    "dev:setup": "npm run dev:setup --workspace @my-better-t-app/backend"
  },
  "dependencies": {
    "@my-better-t-app/env": "*",
    "dotenv": "^17.2.2",
    "zod": "^4.1.13"
  },
  "devDependencies": {
    "@my-better-t-app/config": "*",
    "typescript": "^5"
  },
  "packageManager": "npm@11.7.0"
}
